version: "3"

services:
  quetre:
    container_name: quetre
    image: quay.io/pussthecatorg/quetre:latest@sha256:bde3184ab3e3b817cbf4383af4b366862abedcd31f05beda048553afc0a741c7
    restart: unless-stopped
    environment:
      - "NODE_ENV=production"
      - "PORT=3000"
    security_opt:
      - no-new-privileges:true
    cap_drop:
      - ALL
    networks:
      - quetre

networks:
  quetre:
    name: quetre
