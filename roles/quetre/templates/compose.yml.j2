version: "3"

services:
  quetre:
    container_name: quetre
    image: quay.io/pussthecatorg/quetre:latest@sha256:e51366f85f8626dd4a3e861e5016f50445ddae21f18a50b9575edcbf08cae776
    restart: unless-stopped
    environment:
      - "NODE_ENV=production"
      - "PORT=3000"
    security_opt:
      - no-new-privileges:true
    cap_drop:
      - ALL
    networks:
      - quetre

networks:
  quetre:
    name: quetre
